		"spacing": {
			"units": ["px", "em", "rem", "vh", "vw", "%"]
		},
		"layout": {
			"contentSize": "1320px"
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [{
					"name": "System",
					"slug": "system",
					"fontFamily": "agenda, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\""
				},
				{
					"name": "Serif",
					"slug": "serif",
					"fontFamily": "agenda, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\""
				},
				{
					"name": "Monospace",
					"slug": "monospace",
					"fontFamily": "Consolas,Menlo,Monaco,\"SF Mono\",\"DejaVu Sans Mono\",\"Roboto Mono\",\"Courier New\",Courier,monospace"
				}
			],
			"fontSizes": [{
					"slug": "extra-small",
					"size": ".5rem",
					"name": "Extra small"
				},
				{
					"slug": "small",
					"size": ".75rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "2rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "2.75rem",
					"name": "XL"
				},
				{
					"slug": "xx-large",
					"size": "4rem",
					"name": "XXL"
				}
			]
		},
		"custom": {
			"h-spacing": "2.375rem",
			"v-spacing": "0rem",
			"button-shadow": "0 8px 8px 0 rgba(0, 0, 0, 0.2)",
			"small-button-shadow": "0 1px 1px 0 rgba(0, 0, 0, 0.2)",
			"button-highlight": "0 0 0 #fff",
			"button-border-color": "rgba(0, 0, 0, 0.05)",
			"button-border-radius": "4px",
			"button-padding": ".3rem 1rem"
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--v-spacing)",
			"margin": {
				"top": "0px",
				"right": "0px",
				"bottom": "0px",
				"left": "0px"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"typography": {
			"fontSize": "1.3rem",
			"fontFamily": "var(--wp--preset--font-family--system)",
			"lineHeight": "1.5rem"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"blocks": {
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"core/button": {
				"color": {
					"background": "transparent",
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"border": {
					"radius": "0px",
					"width": "0px",
					"style": "solid",
					"color": "none"
				},
				"spacing": {
					"padding": {
						"top": "0.3rem",
						"right": "0rem",
						"bottom": "0.3rem",
						"left": "0rem"
					}
				}
			},
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			},
			"core/post-navigation-link": {
				"spacing": {
					"margin": {
						"top": "calc(var(--wp--custom--v-spacing) * 3)",
						"bottom": "var(--wp--custom--v-spacing)"
					}
				}
			},
			"core/query-pagination": {
				"spacing": {
					"margin": {
						"top": "calc(var(--wp--custom--v-spacing) * 3) !important"
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			}
		}
	}
}